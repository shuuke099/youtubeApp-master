{
	"name": "youtubeapp-master",
	"version": "1.0.0",
	"description": "Mern-stack youtube application",
	"main": "server.js",
	"scripts": {
		"start": "node server.js",
		"start:pro": "set NODE_ENV=production && nodemon server.js",
		"client-install": "npm install --prefix client",
		"server": "nodemon server.js",
		"client": "npm start --prefix client",
		"dev": "concurrently \"npm run server\" \"npm run client\"",
		"heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
	},
	"author": "Mo shuuke",
	"license": "ISC",
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.19.0",
		"compression": "^1.7.4",
		"concurrently": "^5.2.0",
		"cookie-parser": "^1.4.5",
		"cors": "^2.8.5",
		"crypto": "^1.0.1",
		"dotenv": "^8.2.0",
		"express": "^4.17.3",
		"express-mongo-sanitize": "^2.0.0",
		"express-rate-limit": "^5.1.3",
		"fluent-ffmpeg": "^2.1.2",
		"helmet": "^3.23.3",
		"hpp": "^0.2.3",
		"jsonwebtoken": "^8.5.1",
		"mongoose": "^5.9.19",
		"morgan": "^1.10.0",
		"multer": "^1.4.2",
		"nodemon": "^2.0.4",
		"sharp": "^0.25.4",
		"slugify": "^1.4.0",
		"validator": "^13.0.0",
		"xss-clean": "^0.1.1"
	},
	"engines": {
		"node": "^10"
	}
}
